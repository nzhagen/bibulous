@Article{Sherborne:1990:RGC,
  author =       "B. S. Sherborne and G. E. Stedman",
  title =        "Recursive generation of {Cartesian} angular momentum
                 coupling trees for {${\rm SO}(3)$}",
  journal =      j-COMP-PHYS-COMM,
  volume =       "59",
  number =       "2",
